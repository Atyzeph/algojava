package fr.boucles;

public class ExerciceBoucleBase {
	public static void main(String[] args) {
		
		// Nombres de 1 à 10
		for (int nb = 1; nb <= 10; nb++) {
			System.out.println(nb);
		}
		
		// Affichage nom et prénom
		for (int name = 1; name <= 20; name++) {
			System.out.println(name + " - Bennetot Clément");
		}
	}
}
